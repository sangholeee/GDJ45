	<!-- 삽입 -->
	<insert id="insertProduct" parameterType="ProductDAO">
		INSERT INTO 
		PRODUCT(PRODUCT_NO, NAME, PRICE, IMAGE)
		VALUES (PRODUCT_SEQ.NEXTVAL, #{name}, #{price}, #{image})
	</insert>
	
	
</mapper>