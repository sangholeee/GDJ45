<mapper namespace="mybatis.mapper.free">

	<select id="selectBoardList" resultType="domain.Board">
		SELECT FREENO, WRITER, TITLE, CONTENT, IP, HIT
		  FROM FREE
		 ORDER BY FREENO DESC
	</select>


</mapper>